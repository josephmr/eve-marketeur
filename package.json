		"vite": "^7.1.9",
		"vite-node": "^3.2.4"
	},
	"dependencies": {
		"@sveltejs/adapter-node": "^5.3.3",
		"d3-scale": "^4.0.2",
		"date-fns": "^4.1.0",
		"mode-watcher": "^1.1.0",
		"pg": "^8.16.3",
		"undici": "^7.16.0",
		"zod": "^4.1.12"
	},
	"pnpm": {
		"onlyBuiltDependencies": [
			"@tailwindcss/oxide",
			"esbuild"
		]
	}
