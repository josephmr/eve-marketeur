		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"@tailwindcss/typography": "^0.5.19",
		"@tailwindcss/vite": "^4.1.14",
		"@types/d3-scale": "^4.0.9",
		"@types/node": "^22.18.8",
		"@types/pg": "^8.15.5",
		"bits-ui": "^2.11.4",
		"clsx": "^2.1.1",
		"csv-parse": "^6.1.0",
		"drizzle-kit": "^0.30.6",
		"drizzle-orm": "^0.40.1",
		"layerchart": "2.0.0-next.27",
		"svelte": "^5.39.10",
		"svelte-check": "^4.3.2",
		"tailwind-merge": "^3.3.1",
		"tailwind-variants": "^3.1.1",
